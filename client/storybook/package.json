{
  "private": true,
  "name": "@sourcegraph/storybook",
  "version": "0.0.1",
  "description": "Sourcegraph Storybook configuration",
  "sideEffects": false,
  "license": "Apache-2.0",
  "main": "./src/index.ts",
  "scripts": {
    "lint:js": "pnpm eslint --cache 'src/**/*.[jt]s?(x)'",
    "start": "TS_NODE_TRANSPILE_ONLY=true start-storybook -p 9001 -c ./src",
    "start:chromatic": "CHROMATIC=true TS_NODE_TRANSPILE_ONLY=true pnpm start-storybook -p 9001 -c ./src",
    "build": "TS_NODE_TRANSPILE_ONLY=true pnpm build-storybook -c ./src",
    "build:webpack-stats": "TS_NODE_TRANSPILE_ONLY=true WEBPACK_DLL_PLUGIN=false pnpm start-storybook -c ./src --smoke-test --webpack-stats-json ./storybook-static --loglevel warn",
    "build:dll-bundle": "TS_NODE_TRANSPILE_ONLY=true pnpm webpack --config ./src/webpack.config.dll.ts --no-stats",
    "start:dll": "TS_NODE_TRANSPILE_ONLY=true WEBPACK_DLL_PLUGIN=true pnpm start-storybook -p 9001 -c ./src",
    "clean:dll": "rm -rf assets/dll-bundle storybook-static/*-stats.json",
    "test": "pnpm jest"
  },
  "dependencies": {
    "@sourcegraph/wildcard": "workspace:*",
    "@sourcegraph/common": "workspace:*",
    "@sourcegraph/http-client": "workspace:*",
    "@sourcegraph/shared": "workspace:*",
    "@storybook/addon-controls": "^6.5.14",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "focus-visible": "^5.2.0",
    "history": "4.5.1",
    "https-browserify": "^1.0.0",
    "jsonc-parser": "^3.0.0",
    "monaco-editor": "^0.24.0",
    "open-color": "^1.8.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@sourcegraph/build-config": "workspace:*",
    "@storybook/addon-a11y": "^6.5.14",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-docs": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/addon-storyshots": "^6.5.14",
    "@storybook/addon-storyshots-puppeteer": "^6.5.14",
    "@storybook/addon-storysource": "^6.5.14",
    "@storybook/addon-toolbars": "^6.5.14",
    "@storybook/addons": "^6.5.14",
    "@storybook/api": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/client-api": "^6.5.14",
    "@storybook/components": "^6.5.14",
    "@storybook/core": "^6.5.14",
    "@storybook/core-events": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@storybook/theming": "^6.5.14",
    "@storybook/core-common": "^6.5.14",
    "@terminus-term/to-string-loader": "^1.1.7-beta.1",
    "chromatic": "^6.11.4",
    "jsdom": "^16.7.0",
    "process": "^0.11.10",
    "signale": "^1.4.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "storybook-addon-designs": "^6.3.1",
    "storybook-dark-mode": "^2.0.4",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-manifest-plugin": "^5.0.0",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.2"
  }
}
